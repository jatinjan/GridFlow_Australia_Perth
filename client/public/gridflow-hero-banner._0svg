            fill="#1F2937"/>
      <path d="M-33,180 L33,180" stroke="#1F2937" stroke-width="2"/>
      <path d="M-28,210 L28,210" stroke="#1F2937" stroke-width="2"/>
    </g>
    
    <!-- Background distant towers -->
    <g transform="translate(1700, 380)">
      <path d="M0,220 L-22,220 L-18,195 L-28,170 L-23,145 L-18,120 L-13,95 L-8,70 L-3,45 L0,0 L3,45 L8,70 L13,95 L18,120 L23,145 L28,170 L18,195 L22,220 Z" 
            fill="#1F2937"/>
      <path d="M-28,170 L28,170" stroke="#1F2937" stroke-width="1.5"/>
    </g>
  </g>
  
  <!-- Power Lines Network -->
  <g opacity="0.7">
    <!-- Top level transmission lines -->
    <path d="M55,600 Q200,580 285,600 Q450,620 565,600 Q750,580 865,600 Q1050,620 1168,610 Q1350,600 1472,610 Q1650,620 1722,620" 
          stroke="#1F2937" stroke-width="1.8" fill="none"/>
    <path d="M55,610 Q200,590 285,610 Q450,630 565,610 Q750,590 865,610 Q1050,630 1168,620 Q1350,610 1472,620 Q1650,630 1722,630" 
          stroke="#1F2937" stroke-width="1.8" fill="none"/>
    <path d="M55,620 Q200,600 285,620 Q450,640 565,620 Q750,600 865,620 Q1050,640 1168,630 Q1350,620 1472,630 Q1650,640 1722,640" 
          stroke="#1F2937" stroke-width="1.8" fill="none"/>
    
    <!-- Lower level transmission lines -->
    <path d="M55,650 Q200,630 285,650 Q450,670 565,650 Q750,630 865,650 Q1050,670 1168,660 Q1350,650 1472,660 Q1650,670 1722,670" 
          stroke="#1F2937" stroke-width="1.8" fill="none"/>
    <path d="M55,660 Q200,640 285,660 Q450,680 565,660 Q750,640 865,660 Q1050,680 1168,670 Q1350,660 1472,670 Q1650,680 1722,680" 
          stroke="#1F2937" stroke-width="1.8" fill="none"/>
    <path d="M55,670 Q200,650 285,670 Q450,690 565,670 Q750,650 865,670 Q1050,690 1168,680 Q1350,670 1472,680 Q1650,690 1722,690" 
          stroke="#1F2937" stroke-width="1.8" fill="none"/>
  </g>
  
  <!-- Professional text overlay -->
  <rect x="0" y="0" width="1920" height="800" fill="url(#textReadability)"/>
  
  <!-- GridFlow logo placement area -->
  <rect x="40" y="40" width="400" height="120" fill="none" stroke="none" opacity="0"/>
  
  <!-- Content area for headlines and CTAs -->
  <rect x="300" y="180" width="1320" height="400" fill="none" stroke="none" opacity="0"/>
</svg>